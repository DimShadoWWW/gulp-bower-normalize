      }
    },
    "dependency4": {
      "main": ["some.js", "some.json"],
      "normalize": {
        "js": ["*.js", "*.json"]
      }
    },
    "dependency5": {
      "main": ["some.js", "some.json"],
      "normalize": {
        "js": "*.js"
      }
    },
    "dependency6": {
      "main": "some/long/path/to/file.js"
    }
  }
}